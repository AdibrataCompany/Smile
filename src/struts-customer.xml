<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="customer" extends="smartdealer-package"
		namespace="/">
		<action name="customerentry"
			class="com.adibrata.smartdealer.action.customer.CustomerAction"
			method="execute">
			<result name="start">/Pages/Customer/paging.jsp</result>
			<result name="SUCCESS">/Pages/Customer/paging.jsp</result>
			<result name="search">/Pages/Customer/paging.jsp</result>
			<result name="end">/Pages/Customer/paging.jsp</result>
			<result name="ERROR">/Pages/Customer/entry.jsp</result>
			<result name="entry">/Pages/Customer/entry.jsp</result>
			<result name="adddetail">/Pages/Customer/entry.jsp</result>
		</action>

	</package>
</struts>
