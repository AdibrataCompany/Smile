<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
 "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="documentCode">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please fill the document code</message>
		</field-validator>
	</field>
	<field name="documentName">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please fill the document name</message>
		</field-validator>
	</field>
	<field name="assettype">
		<field-validator type="requiredstring">
			<param name="trim">true</param>
			<message>Please fill the asset type</message>
		</field-validator>
	</field>
</validators>