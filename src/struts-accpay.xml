<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="accpay" extends="smartdealer-package" namespace="/">
		<action name="accpay"
			class="com.adibrata.smartdealer.action.accpay.AccPayAction" method="execute">
			<result name="start">/Pages/AccPay/Selection/ApSelection.jsp
			</result>
			<result name="success">/Pages/AccPay/Selection/ApSelection.jsp
			</result>
			<result name="search">/Pages/AccPay/Selection/ApSelection.jsp
			</result>
			<result name="end">/Pages/AccPay/Selection/ApSelection.jsp
			</result>
			<result name="error">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
			<result name="entry">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
			<result name="adddetail">/Pages/AccPay/Selection/ApSelectionEntry.jsp
			</result>
		</action>
		<action name="pvdisburse"
			class="com.adibrata.smartdealer.action.accpay.PaymentVoucherAction"
			method="execute">
			<result name="start">/Pages/AccPay/PaymentVoucher/PVDisburse.jsp
			</result>
			<result name="success">/Pages/AccPay/PaymentVoucher/PVDisburse.jsp
			</result>
			<result name="search">/Pages/AccPay/PaymentVoucher/PVDisburse.jsp
			</result>
			<result name="end">/Pages/AccPay/PaymentVoucher/PVDisburse.jsp
			</result>
			<result name="error">/Pages/AccPay/PaymentVoucher/PVDisburseEntry.jsp
			</result>
			<result name="entry">/Pages/AccPay/PaymentVoucher/PVDisburseEntry.jsp
			</result>
			<result name="adddetail">/Pages/AccPay/PaymentVoucher/PVDisburseEntry.jsp
			</result>
		</action>

	</package>
</struts>