<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="suspend" extends="smartdealer-package"
		namespace="/">
		<action name="suspendreceive"
			class="com.adibrata.smartdealer.action.accmnt.SuspendReceiveAction"
			method="execute">
			
			<result name="start">/Pages/Suspend/SuspendReceive/SuspendReceive.jsp
			</result>
			<result name="success">/Pages/Suspend/SuspendReceive/SuspendReceive.jsp
			</result>
			<result name="error">/Pages/Suspend/SuspendReceive/SuspendReceive.jsp
			</result>
			<result name="save">/Pages/Suspend/SuspendReceive/SuspendReceive.jsp
			</result>
			<result name="end">/Pages/Suspend/SuspendReceive/SuspendReceive.jsp
			</result>
		</action>
		<action name="suspendallocation"
			class="com.adibrata.smartdealer.action.accmnt.SuspendAllocationAction"
			method="execute">
			<result name="start">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="success">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="error">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>

			<result name="search">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="entry">/Pages/Suspend/SuspendReverse/entry.jsp
			</result>
			<result name="end">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
		</action>
		<action name="suspendreverse"
			class="com.adibrata.smartdealer.action.accmnt.SuspendReverseAction"
			method="execute">
			<result type="json">
				<param name="excludeNullProperties">true</param>
				<param name="noCache">true</param>
			</result>
			<result name="start">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="success">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="error">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>

			<result name="search">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="entry">/Pages/Suspend/SuspendReverse/entry.jsp
			</result>
			<result name="end">/Pages/Suspend/SuspendReverse/paging.jsp
			</result>
			<result name="first">/Pages/Suspend/SuspendReverse/paging.jsp</result>
			<result name="prev">/Pages/Suspend/SuspendReverse/paging.jsp</result>
			<result name="next">/Pages/Suspend/SuspendReverse/paging.jsp</result>
			<result name="last">/Pages/Suspend/SuspendReverse/paging.jsp</result>
		</action>
	</package>
</struts>