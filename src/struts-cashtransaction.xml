<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="cashtransaction" extends="smartdealer-package"
		namespace="/">
		<action name="advancecashrequest"
			class="com.adibrata.smartdealer.action.cashtransaction.AdvanceRequestAction"
			method="execute">
			<result name="start">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="success">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="error">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="search">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="end">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="entry">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
			<result name="adddetail">/Pages/CashTransaction/AdvanceTransaction/AdvanceRequest.jsp
			</result>
		</action>
		<action name="advancecashreverse"
			class="com.adibrata.smartdealer.action.cashtransaction.AdvanceRequestAction"
			method="execute">
			<result name="start">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
			<result name="success">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
			<result name="search">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
			<result name="end">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
			<result name="error">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
			<result name="entry">/Pages/CashTransaction/AdvanceTransaction/AdvanceReverse.jsp
			</result>
		</action>
		<action name="paymentrequest"
			class="com.adibrata.smartdealer.action.cashtransaction.PaymentRequestAction"
			method="execute">
			<result name="start">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="success">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="search">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="end">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="error">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="entry">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
			<result name="adddetail">/Pages/CashTransaction/PaymentRequest/PaymentRequest.jsp
			</result>
		</action>
		<action name="pettycash"
			class="com.adibrata.smartdealer.action.cashtransaction.PettyCashAction"
			method="execute">
			<result name="start">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="success">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="search">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="end">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="error">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="entry">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
			<result name="adddetail">/Pages/CashTransaction/PettyCash/PettyCash.jsp
			</result>
		</action>

	</package>
</struts>